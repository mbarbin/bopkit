; dune file generated by '%{bin:bopkit} fmt gen-dune' -- do not edit.

(rule
 (with-stdout-to
  and.bop.pp.output
  (bash "%{bin:bopkit} fmt file %{dep:and.bop}")))

(rule
 (alias fmt)
 (action
  (diff and.bop and.bop.pp.output)))

(rule
 (with-stdout-to
  out.bop.pp.output
  (bash "%{bin:bopkit} fmt file %{dep:out.bop}")))

(rule
 (alias fmt)
 (action
  (diff out.bop out.bop.pp.output)))
