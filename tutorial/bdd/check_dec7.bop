#include "dec7.bop"

#define FILENAME "dec7.txt"
#define AD 4
#define WL 7

Test() = ()
where
  a:[AD] = external("counter.exe -N %{AD}");
  block[1]:[WL] = Block(a:[AD]);
  block[2]:[WL] = external("bopkit simu dec7-tree.bop -p", a:[AD]);
  for i = 1 to 2
    external("bopkit bdd checker -AD %{AD} -WL %{WL} -f %{FILENAME}",
      a:[AD],
      block[i]:[WL]);
  end for;
end where;
