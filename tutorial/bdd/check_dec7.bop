#include "dec7.bop"

#define FILENAME "dec7.txt"
#define AD 4
#define WL 7

Test() = ()
where
  a:[AD] = external("counter.exe -N %{AD}");
  bloc[1]:[WL] = Bloc(a:[AD]);
  bloc[2]:[WL] = external("bopkit simu dec7-tree.bop -p", a:[AD]);
  for i = 1 to 2
    external("bopkit bdd checker -AD %{AD} -WL %{WL} -f %{FILENAME}",
      a:[AD],
      bloc[i]:[WL]);
  end for;
end where;
